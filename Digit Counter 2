import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int count=0;
        int num=sc.nextInt();
        
        while(num!=0){
            
            count++;
            
            num=num/10;
        }
      System.out.println(count);      
            
    }
}
